{{#extends 'base'}}
    {{#block 'content'}}
        <table class="table">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Title</th>
                    <th>Set</th>
                    <th>Sales</th>
                    <th>Stock</th>
                    <th>Date</th>
                </tr>
            </thead>
            <tbody>
                {{#each posters}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.cost}}</td>
                    <td>{{this.date}}</td>
                    <td>{{this.stock}}</td>
                    <td>{{this.height}}</td>
                    <td>{{this.width}}</td>
                    <td>{{}}
                </tr>
                {{/each}}
            </tbody>
        </table>
    {{/block}}
{{/extends}}